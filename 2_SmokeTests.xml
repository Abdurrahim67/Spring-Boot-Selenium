<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Egeko">
    <test name="Smoke Tests">
        <groups>
            <run>
                <include name="smoke"></include>
            </run>
        </groups>
        <packages>
            <package name="automation.*"></package>
        </packages>
    </test>
</suite>

